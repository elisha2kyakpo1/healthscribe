<div class="d-flex entries signup_div pb-5 justify-content-center">
  <div class="w-50">
    <div class="bg-white item-cont pb-5">
      <div class='w-100 header'>
        <div class='container d-flex flex-column justify-content-end align-items-center'>
          <div class='d-flex gap-2 gap-md-4 align-items-center justify-content-between'>
            <h2 class='para text-white mb-0'>Entries</h2>
          </div>
        </div>
      </div>
      <div class="px-5">
        <div class="items d-flex py-3 align-items-center">
          <%= svg_tag "Food"%>
          <%= link_to 'Meal/food', foods_path, class: '' %>
        </div>
        <div class="d-flex items py-3 align-items-center">
          <%= svg_tag "Drink"%>
          <%= link_to 'Drink', drinks_path, class: '' %>
        </div>

        <div class="d-flex items py-3 align-items-center">
          <%= svg_tag "Mood"%>
          <%= link_to 'Mood', moods_path, class: '' %>
        </div>

        <div class="d-flex items py-3 align-items-center">
          <%= svg_tag "Medication"%>
          <%= link_to 'Medication', medications_path, class: '' %>
        </div>

        <div class="d-flex items py-3 align-items-center">
          <%= svg_tag "Symptom"%>
          <%= link_to 'Symptom', symptoms_path, class: '' %>
        </div>
      </div>
      <div class="cancel">
        <%= link_to '<i class="fa-sharp fa-solid fa-xmark"></i>'.html_safe, symptoms_path %>
      </div>
    </div>
  </div>
</div>
