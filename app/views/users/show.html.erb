<div class="position-relative">
  <div class="sign-head">
    <%= render "layouts/menu_bar" %>
      <div class='d-flex gap-2 gap-md-4 justify-content-between'>
        <div class="d-flex align-items-center active-link w-50 active">
          <%= svg_tag 'Timeline_Selected', class: "timeline" %>
          <%= link_to 'Timeline', user_path(current_user), class: "time-insight py-3" %>
        </div>
        <div class="d-flex align-items-center active-rink">
          <%= svg_tag 'Insights_Selected', class: "timeline" %>
          <%= link_to 'Insight', entries_insight_path, class: "time-insight py-3" %>
        </div>
      </div>
    </div>
  </div>
  <div class ="syptom_div">
    <div class ="symptom_data">
    <% if @my_symptoms.any? %>
      <% @my_symptoms.each do |symptom| %>
        <section class="gradient-custom-5">
          <div class="container">
            <div class="main-timeline-5">
              <div class="timeline-5 right-5">
                <div class="">
                  <div class="card-body p-4 hoverWrapper">
                    <%= svg_tag "Symptom", class: "show-icon"%>
                    <h6 class="datetime text-dark">Symptom</h6>
                    <p class="item-name"><%= symptom.intensity %></p>
                    <article class="d-flex flex-column align-items-center position-absolute datetime">
                      <p class="item-name"><%= symptom.time.strftime("%I:%M%p") %></p>
                    </article>                    <p class="mt-2 mb-0"><%= symptom.comment %></p>
                    <div class="hoverContent rounded-1 card-action">
                      <%= turbo_frame_tag dom_id(current_user, :thumbnail), target: "_top", loading: :lazy, src: user_hover_symptom_path(current_user) do %>
                        Loading...
                      <% end %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
      <% end %> 
    <% end %>   
  </div>

  <div class ="syptom_div">
    <div class ="symptom_data">
    <% if @my_foods.any? %>
      <% @my_foods.each do |food| %>
