                    <article class="d-flex flex-column align-items-center position-absolute datetime">
                      <p class="item-name"><%= food.time.strftime("%I:%M%p") %></p>
                    </article>                    <p class="mt-2 mb-0"><%= food.comment %></p>
                    <div class="hoverContent rounded-1 card-action">
                      <%= turbo_frame_tag dom_id(current_user, :thumbnail), target: "_top", loading: :lazy, src: user_hover_food_path(current_user) do %>
                        Loading...
                      <% end %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
      <% end %> 
    <% end %>   
  </div>
