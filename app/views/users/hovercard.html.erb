<%= turbo_frame_tag dom_id(current_user, :thumbnail) do %>
  <div class="">
    <% @items.each do |item| %>
      <% if item.is_a?(Symptom) %>
        <small><%= time_ago_in_words(item.created_at) %> ago </small>
        <div class="d-flex justify-content-between align-items-center">
          <%= link_to "Edit", edit_symptom_path(item), class:"editbtn" %>
          <div class="deletebtn">
            <%= button_to "Delete", symptom_path(item), method: :delete, form: {data: {turbo_confirm: 'Are you sure?'}} %>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
<% end %>